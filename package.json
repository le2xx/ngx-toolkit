{
  "name": "ngx-toolkit",
  "version": "16.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run test:lib:device && npm run test:lib:cache && npm run test:lib:cookie && npm run test:lib:logger && npm run test:lib:spring && npm run test:lib:utils",
    "test:lib:device": "ng test device --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "test:lib:cache": "ng test cache --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "test:lib:cookie": "ng test cookie --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "test:lib:logger": "ng test logger --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "test:lib:spring": "ng test spring --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "test:lib:utils": "ng test utils --watch false --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:libs": "npm run build:lib:device && npm run build:lib:cache && npm run build:lib:cookie && npm run build:lib:logger && npm run build:lib:spring && npm run build:lib:utils",
    "build:lib:device": "ng build device --configuration production",
    "build:lib:cache": "ng build cache --configuration production",
    "build:lib:cookie": "ng build cookie --configuration production",
    "build:lib:logger": "ng build logger --configuration production",
    "build:lib:spring": "ng build spring --configuration production",
    "build:lib:utils": "ng build utils --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/router": "^16.2.7",
    "rxjs": "~6.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "~16.2.0",
    "@angular-eslint/eslint-plugin-template": "~16.2.0",
    "@angular-eslint/schematics": "~16.2.0",
    "@angular-eslint/template-parser": "~16.2.0",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@angular/language-service": "^16.2.7",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "codelyzer": "^6.0.2",
    "eslint": "^8.39.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.2.3",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~5.1.6"
  }
